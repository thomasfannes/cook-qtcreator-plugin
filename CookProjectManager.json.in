{
    \"Name\" : \"CookPlugin\",
    \"Version\" : \"1.0.0\",
    \"CompatVersion\" : \"1.0.0\",
    \"Vendor\" : \"Thomas Fannes\",
    \"Copyright\" : \"\",
    \"License\" : \"LGPLv3\",
    \"Description\" : \"A plugin for using Cook in QtCreator\",
    \"Url\" : \"http://www.mycompany.com\",
    $$dependencyList,

    \"Mimetypes\" : [
        \"<?xml version=\'1.0\'?>\",
        \"<mime-info xmlns=\'http://www.freedesktop.org/standards/shared-mime-info\'>\",
        \"  <mime-type type=\'text/x-chai\'>\",
        \"    <sub-class-of type=\'text/plain\'/>\",
        \"    <comment>Chai script file</comment>\",
        \"    <glob pattern=\'*.chai\'/>\",
        \"  </mime-type>\",
        \"  <mime-type type=\'text/x-cook-project\'>\",
        \"    <sub-class-of type=\'text/x-chai\'/>\",
        \"    <comment>Cook recipe file</comment>\",
        \"    <glob pattern=\'recipes.chai\'/>\",
        \"  </mime-type>\",
        \"</mime-info>\"
    ]
}
